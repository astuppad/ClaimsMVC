@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<html>
<head>
    @*<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous">*@
    <script src="https://ajax.aspnetcdn.com/ajax/jQuery/jquery-3.3.1.min.js"></script>
</head>
<body>
    <div>
    </div>
    <div class="accordion" id="accordionExample">
        <div class="card">
            <div class="card-header" id="headingOne">
                <h5 class="mb-0">
                    <button class="btn btn-link" type="button" data-toggle="collapse" data-target="#policyDetails" aria-expanded="true" aria-controls="collapseOne">
                        Policy Details
                    </button>
                </h5>
            </div>

            <div id="policyDetails" class="collapse " aria-labelledby="headingOne" data-parent="#accordionExample">
                <div class="card-body">
                    @{ Html.RenderPartial("~/Views/PolicyDetails_PD/Create.cshtml", new PolicyDetails_PD()); }
                </div>
            </div>
        </div>
        <div class="card">
                <div class="card-header" id="headingOne">
                    <h5 class="mb-0">
                        <button class="btn btn-link" type="button" data-toggle="collapse" data-target="#incident" aria-expanded="true" aria-controls="collapseOne">
                            incident Details
                        </button>
                    </h5>
                </div>

                <div id="incident" class="collapse " aria-labelledby="headingOne" data-parent="#accordionExample">
                    <div class="card-body">
                        @{ Html.RenderPartial("~/Views/Incident_IDT/Create.cshtml", new Incident_IDT()); }
                    </div>
                </div>
            </div>
            <div class="card">
                <div class="card-header" id="headingOne">
                    <h5 class="mb-0">
                        <button class="btn btn-link" type="button" data-toggle="collapse" data-target="#lossLocation" aria-expanded="true" aria-controls="collapseOne">
                            Loss Location Details
                        </button>
                    </h5>
                </div>

                <div id="lossLocation" class="collapse " aria-labelledby="headingOne" data-parent="#accordionExample">
                    <div class="card-body">
                        @{ Html.RenderPartial("~/Views/LossLocation_LL/Create.cshtml", new LossLocation_LL()); }
                    </div>
                </div>
            </div>
            <div class="card">
                <div class="card-header" id="headingOne">
                    <h5 class="mb-0">
                        <button class="btn btn-link" type="button" data-toggle="collapse" data-target="#informant" aria-expanded="true" aria-controls="collapseOne">
                            Informant Details
                        </button>
                    </h5>
                </div>

                <div id="informant" class="collapse " aria-labelledby="headingOne" data-parent="#accordionExample">
                    <div class="card-body">
                        @{ Html.RenderPartial("~/Views/Informant_INF/Create.cshtml", new Informant_INF()); }
                    </div>
                </div>
            </div>
            <div class="card">
                <div class="card-header" id="headingOne">
                    <h5 class="mb-0">
                        <button class="btn btn-link" type="button" data-toggle="collapse" data-target="#claimContact" aria-expanded="true" aria-controls="collapseOne">
                            Claim Contact Details
                        </button>
                    </h5>
                </div>

                <div id="claimContact" class="collapse " aria-labelledby="headingOne" data-parent="#accordionExample">
                    <div class="card-body">
                        @{ Html.RenderPartial("~/Views/Informant_INF/Create.cshtml", new Informant_INF()); }
                    </div>
                </div>
            </div>
            <div class="card">
                <div class="card-header" id="headingOne">
                    <h5 class="mb-0">
                        <button class="btn btn-link" type="button" data-toggle="collapse" data-target="#catastrophe" aria-expanded="true" aria-controls="collapseOne">
                            Catastrophe Details
                        </button>
                    </h5>
                </div>

                <div id="catastrophe" class="collapse " aria-labelledby="headingOne" data-parent="#accordionExample">
                    <div class="card-body">
                       @{ Html.RenderPartial("~/Views/Catastrophe_CPE/Create.cshtml", new Catastrophe_CPE()); }
                    </div>
                </div>
            </div>
            <div class="card">
                <div class="card-header" id="headingOne">
                    <h5 class="mb-0">
                        <button class="btn btn-link" type="button" data-toggle="collapse" data-target="#claimDecision" aria-expanded="true" aria-controls="collapseOne">
                            Claim Decision
                        </button>
                    </h5>
                </div>

                <div id="claimDecision" class="collapse " aria-labelledby="headingOne" data-parent="#accordionExample">
                    <div class="card-body">
                        @{ Html.RenderPartial("~/Views/ClaimDecision_CD/Create.cshtml", new ClaimDecision_CD()); }
                    </div>
                </div>
            </div>
    </div>
    <button class="btn btn-success" id="saveClaims">Save</button>
</body>
</html>
